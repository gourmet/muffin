{
    "name": "gourmet/muffin"
    , "description": "FactoryMuffin support for CakePHP 3"
    , "type": "cakephp-plugin"
    , "keywords": [
        "cakephp"
        , "gourmet"
        , "factory muffin"
    ]
    , "homepage": "https://github.com/gourmet/muffin"
    , "license": "MIT"
    , "authors": [
        {
            "name": "Jad Bitar"
            , "homepage": "http://jadb.io"
            , "role": "Author"
        }
        , {
            "name": "Others"
            , "homepage": "https://github.com/gourmet/muffin/graphs/contributors"
        }
    ]
    , "support": {
        "issues": "https://github.com/gourmet/muffin/issues"
        , "source": "https://github.com/gourmet/muffin"
    }
    , "require": {
        "cakephp/cakephp": "3.0.*-dev"
        , "league/factory-muffin": "~2.1"
    }
    , "require-dev": {
        "phpunit/phpunit": "4.1.*"
    }
    , "autoload": {
        "psr-4": {
            "Gourmet\\Muffin\\": "src"
        }
    }
    , "autoload-dev": {
        "psr-4": {
            "Gourmet\\Muffin\\Test\\": "tests"
        }
    }
}
